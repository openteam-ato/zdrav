<%= simple_form_for [:eco, @coupon] do |f| %>
  <%= f.input :created_on, :start_year => Time.zone.now.year, :end_year => Time.zone.now.year - 10 if @coupon.created? %>
  <%= f.input :mi_title, :autofocus => true, :autofocus => true,
    :input_html => { :class => 'js-autocomplete',
                     :data => { :url => search_eco_medical_institutions_path },
                     :value => @coupon.medical_institution_title  } if @coupon.issued?%>
  <%= f.input :issued_on, :start_year => Time.zone.now.year, :end_year => Time.zone.now.year - 10 if @coupon.issued? %>
  <%= f.submit 'Добавить', :class => [:btn, 'btn-default'] %>
  <%= link_to 'Отмена', :back %>
<% end %>
