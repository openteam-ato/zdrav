  <h1><%= @page.title %></h1>

<div class="row">
  <%= simple_form_for @declaration_support, url: declaration_supports_path do |f| %>
    <div class='col-xs-6'>
      <%= f.input :name %>
      <%= f.input :surname %>
      <%= f.input :patronymic %>
      <%= f.input :job %>
      <%= f.input :email %>
      <%= f.input :agreement %>
      <%= f.submit %>
    </div>
  <% end %>
</div>
